@using HomeWork.Models;

    <table class="table" >
        <tr>
            <th>#</th>
            <th>類別</th>
            <th>日期</th>
            <th>金額</th>
            <th>備註</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MoneyCount</td>
                <td>@item.MoneyType</td>
                <td>@item.MoneyAmount</td>
                <td>@item.MoneyDate</td>
                <td>@item.MoneyNote</td>
            </tr>
        }



    </table>

